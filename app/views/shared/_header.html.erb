<header class='top-page-header'>
    <div class='search-bar-contents'>
      <%= link_to image_tag("motivate_diary_logo.png",size: '700x150', class:"motivate-diary-icon"), "/" %><br/>
    </div>
      
        <ul class='lists'>
          <% if user_signed_in? %>
          <li><%= link_to "#{current_user.nickname}さん", class: "user-nickname" %></li>
          <li><%= link_to "ログアウト", destroy_user_session_path, method: :delete, class: "logout" %>
        <% else %>
          <li><%= link_to 'ログイン', new_user_session_path, class: "login" %>
          <li><%= link_to '新規登録', new_user_registration_path, class: "sign-up" %>
        <% end %>
        </ul>
        </div>
      </div>
</header>